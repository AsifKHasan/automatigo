# find and replace patterns
  # comma - space after, no space before
  - find                : ','
    replace-with        : ', '
  - find                : ' ,'
    replace-with        : ','

  # period - space after, no space before
  - find                : '.'
    replace-with        : '. '
  - find                : ' .'
    replace-with        : '.'

  # semicolon - space after, no space before
  - find                : ';'
    replace-with        : '; '
  - find                : ' ;'
    replace-with        : ';'

  # colon - space after, no space before
  - find                : ':'
    replace-with        : ': '
  - find                : ' :'
    replace-with        : ':'

  # opening brace - space before (except when it is a "), no space after
  - find                : '('
    replace-with        : ' ('
  - find                : '( '
    replace-with        : '('
  - find                : '" ('
    replace-with        : '"('

  # # closing brace - space after (except when " or period or comma), no space before
  - find                : ')'
    replace-with        : ') '
  - find                : ' )'
    replace-with        : ')'
  - find                : ') .'
    replace-with        : ').'
  - find                : '. )'
    replace-with        : '.)'
  - find                : ') ,'
    replace-with        : '),'
  - find                : ', )'
    replace-with        : ',)'
  - find                : ') "'
    replace-with        : ')"'

  # remove double space, periods
  - find                : '  '
    replace-with        : ' '
  - find                : '..'
    replace-with        : '.'

  # remove nultiline
  - find                : \n
    replace-with        : ' '

  # remove single dash
  - find                : '-'
    replace-with        : ''
    entire-cell         : True
