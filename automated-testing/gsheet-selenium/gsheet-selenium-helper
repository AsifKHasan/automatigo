from pygsheets.custom_types import *
import pygsheets
import pandas as pd
import numpy as np

_CREDENTIAL = '../conf/credential.json'
_GOOGLE_SHEET_NAME = 'MEM__test-case__create-meeting'
_G = pygsheets.authorize(service_account_file=_CREDENTIAL)
_sheet = _G.open(_GOOGLE_SHEET_NAME)

ws_title = 'workflow'
self.info('reading worksheet: {0}'.format(ws_title))
ws = _sheet.worksheet('title', ws_title)
df = ws.get_as_df(has_header=True, index_colum=None, start='B2', end='H{}'.format(ws.rows))


df.process = pd.Series(df.process).fillna(method='ffill')
df.work = pd.Series(df.work).fillna(method='ffill')
